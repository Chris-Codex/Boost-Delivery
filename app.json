{
  "expo": {
    "name": "order-foodie",
    "slug": "order-foodie",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/"
    }
  }
}
